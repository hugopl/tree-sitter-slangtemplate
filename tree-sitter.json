{
  "grammars": [
    {
      "name": "slangtemplate",
      "camelcase": "SlangTemplate",
      "scope": "text.slang",
      "path": ".",
      "file-types": [
        "slang"
      ]
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "A tree-sitter based parser for slang template language",
    "links": {
      "repository": "https://github.com/hugopl/tree-sitter-slang"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
